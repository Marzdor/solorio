{
  "name": "solorio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://marzdor.github.io/solorio/",
  "dependencies": {
    "normalize.css": "^8.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
